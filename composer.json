{
    "name": "spatie/simple-excel",
    "description": "Easily read and write simple Excel and CSV files",
    "keywords": [
        "spatie",
        "simple-excel"
    ],
    "homepage": "https://github.com/spatie/simple-excel",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "box/spout": "^3.0",
        "illuminate/support": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "spatie/phpunit-snapshot-assertions": "^2.1",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\SimpleExcel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\SimpleExcel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Spatie\\Skeleton\\SkeletonFacade"
            }
        }
    }
}
